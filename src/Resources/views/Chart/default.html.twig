{% set report = data.report %}
{% set labels = data.labels %}
{% set values = data.values %}

{% block content %}
    <canvas id="{{ report.code }}" data-type="{{ configuration.type }}" data-labels="{% for items in values %}{{ items[0] }}{% if loop.index != loop.length %};{% endif %}{% endfor %}" data-values="{% for items in values %}{{ items[items|length-1] }}{% if loop.index != loop.length %};{% endif %}{% endfor %}" height="400"></canvas>
{% endblock %}
